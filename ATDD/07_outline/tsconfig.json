{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "declaration": false,
    "removeComments": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types"],
    "types": ["node", "cucumber", "protractor"],

    "mapRoot": "typeScript",
    "outDir": "typeScript",
    "sourceMap": true
  },
  "exclude": ["node_modules", "typescript"]
}
